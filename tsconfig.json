{
  "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "lib": [
          "esnext",
      ],
      "outDir": "dist/",
      "declaration": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "strict": true,
  },
  "include": [
      "src/**/*.ts",
  ],
  "exclude": [
      "node_modules"
  ]
}
